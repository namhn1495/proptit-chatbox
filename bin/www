      g += '<img src="http://blog.uhm.vn/emo/yahoo/22.gif">';
      i = i + 1;
    } else if ((i + 1) < message.length && message[i + 0] == '=' && message[i + 1] == ';') {
      g += '<img src="http://blog.uhm.vn/emo/yahoo/27.gif">';
      i = i + 1;
    } else

    {
      g += message[i];
    }

  }
